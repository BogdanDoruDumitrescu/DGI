<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #696969;" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.fis.maven.Controllers.LoginClientController">
    <children>
        <Button layoutX="66.0" layoutY="99.0" mnemonicParsing="false" onAction="#butonBack" style="-fx-background-color: #FF8C00;" text="Back" />
        <TextField fx:id="usernameField" layoutX="220.0" layoutY="156.0" promptText="Enter your username" />
        <PasswordField fx:id="passwordField" layoutX="220.0" layoutY="204.0" promptText="Enter your password" />
        <Label fx:id="user" layoutX="147.0" layoutY="161.0" text="Username:" textFill="DARKORANGE">
            <font>
                <Font size="14.0" />
            </font>
        </Label>
        <Label fx:id="pass" layoutX="149.0" layoutY="209.0" text="Password:" textFill="DARKORANGE">
            <font>
                <Font size="14.0" />
            </font>
        </Label>
        <Button layoutX="256.0" layoutY="259.0" mnemonicParsing="false" onAction="#login" prefHeight="27.0" prefWidth="88.0" style="-fx-background-color: #FF8C00;" text="Login" />
        <Label fx:id="id" alignment="CENTER" layoutX="221.0" layoutY="97.0" prefHeight="30.0" prefWidth="158.0" text="Welcome, Client!" textFill="DARKORANGE">
            <font>
                <Font size="20.0" />
            </font>
        </Label>
      <Label fx:id="error" layoutX="212.0" layoutY="317.0" text="Incorrect username or password!" textFill="DARKORANGE" />


    </children>
</AnchorPane>
